<?xml version="1.0"?>
<root>
    <item>
        <name>Emacs Dvorak</name>
        <item>
            <name>M-h</name>
            <identifier>private.m_h</identifier>
            <not>EMACS</not>

            <autogen>
                __KeyToKey__
                KeyCode::J,      ModifierFlag::OPTION_L,
                KeyCode::DELETE, ModifierFlag::OPTION_L
            </autogen>
        </item>
        <item>
            <name>C-a and C-e</name>
            <identifier>private.c_a_and_c_e</identifier>
            <not>EMACS</not>

            <autogen>
                __KeyToKey__
                KeyCode::A,           ModifierFlag::CONTROL_L,
                KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L
            </autogen>
            <autogen>
                __KeyToKey__
                KeyCode::D,           ModifierFlag::CONTROL_L,
                KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L
            </autogen>
        </item>
        <item>
            <name>C-v and M-v</name>
            <identifier>private.c_v_and_m_v</identifier>
            <not>EMACS</not>

            <autogen>
                __KeyToKey__
                KeyCode::DOT, ModifierFlag::CONTROL_L,
                KeyCode::PAGEDOWN
            </autogen>
            <autogen>
                __KeyToKey__
                KeyCode::DOT, ModifierFlag::OPTION_L,
                KeyCode::PAGEUP
            </autogen>
        </item>
        <item>
            <name>C-y, M-w and C-w</name>
            <identifier>private.c_y_m_w_and_c_w</identifier>
            <not>EMACS</not>

            <autogen>
                __KeyToKey__
                KeyCode::T, ModifierFlag::CONTROL_L,
                KeyCode::DOT, ModifierFlag::COMMAND_L
            </autogen>
            <autogen>
                __KeyToKey__
                KeyCode::COMMA, ModifierFlag::OPTION_L,
                KeyCode::I,     ModifierFlag::COMMAND_L
            </autogen>
            <autogen>
                __KeyToKey__
                KeyCode::COMMA, ModifierFlag::CONTROL_L,
                KeyCode::B,     ModifierFlag::COMMAND_L
            </autogen>
        </item>

        <item>
            <name>C-s</name>
            <identifier>private.c_s</identifier>
            <not>EMACS</not>

            <autogen>
                __KeyToKey__
                KeyCode::SEMICOLON, ModifierFlag::CONTROL_L,
                KeyCode::Y, ModifierFlag::COMMAND_L
            </autogen>
        </item>
    </item>
</root>

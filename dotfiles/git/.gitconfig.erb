[user]
  name = <%= @config.real_name %>
  email = <%= @config.email %>
[github]
  user = <%= @config.username %>
  token = <%= @config.github_token %>
[color]
  diff = auto
  branch = auto
  interactive = auto
  status = auto
[core]
  autocrlf = input
  excludesfile = ~/.gitignore_global
[help]
  autocorrect = 50
[apply]
  whitespace = nowarn
[push]
  default = current

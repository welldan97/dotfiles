"*":
  "advanced-open-file":
    createDirectories: true
  "api-docs":
    "angularjs~1": {}
    "lodash~4": true
    react: true
  "atom-beautify":
    json:
      beautify_on_save: true
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    similarSuggestionRemoval: "textOrSnippet"
  autosave:
    enabled: true
  "bracket-matcher":
    highlightMatchingLineNumber: true
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
        ".flowconfig"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "language-gfm"
      "metrics"
      "welcome"
      "Hydrogen"
      "autocomplete"
      "lcov-info"
      "image-view"
      "vim-mode-plus"
      "encoding-selector"
      "atom-typescript"
    ]
    packagesWithKeymapsDisabled: [
      "scratch"
      "api-docs"
      "github"
      "open-on-github"
      "git-diff"
      "atomic-emacs"
      "sourcerer"
      "grammar-selector"
      "link"
      "prettier-atom"
      "atom-typescript"
      "recent-files-fuzzy-finder"
    ]
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "emacs-syntax"
    ]
  editor:
    invisibles:
      space: " "
    preferredLineLength: 80
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapHangingIndent: 2
  "exception-reporting":
    userId: "d2beda34-0972-47cb-be2c-5b936aa50ed3"
  "git-diff":
    showIconsInEditorGutter: true
  helicopter:
    keyboardLayout: "Programmer Dvorak"
  "highlight-column":
    enabled: false
    opacity: 0.025
  "highlight-line":
    enableBackgroundColor: false
  "highlight-selected":
    highlightBackground: true
  "ide-typescript":
    javascriptSupport: false
  "lcov-info": {}
  "linter-eslint":
    disabling: {}
  "linter-shellcheck":
    enableNotice: true
  "linter-ui-default":
    panelHeight: 258
    showPanel: true
  minimap: {}
  nuclide:

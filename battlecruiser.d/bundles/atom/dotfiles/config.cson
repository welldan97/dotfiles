  "open-recent":
    maxRecentDirectories: 16
    maxRecentFiles: 80
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
      excludedGlobs: [
        "*.md"
        "*.json"
        "*.html"
      ]
      isDisabledIfNoConfigFile: true
      respectEslintignore: false
      showInStatusBar: true
    prettierEslintOptions: {}
    prettierOptions:
      printWidth: 80
    useEslint: true
    useStylelint: true
  "recent-files-fuzzy-finder":
    maxFilesToRemember: 100
  "scroll-through-time":
    direction: "right"
    modifierKey: "altKey"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  tabs:
    displayMruTabList: false
    enableMruTabSwitching: false
    enableVcsColoring: true
  "tree-view":
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
".json.source":
  editor:
    showIndentGuide: false
".reason.source":
  editor:
    preferredLineLength: 110

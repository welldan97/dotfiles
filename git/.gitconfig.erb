[user]
	name = <%= @config.name %>
	email = <%= @config.email %>
[github]
	user = <%= @config.username %>
	token = <%= @config.github_token %>
[alias]
  co = checkout
[color]
	diff = auto
	branch = auto
	interactive = auto
	status = auto
[core]
	editor = pico
	autocrlf = input
	excludesfile = ~/.gitignore_global
[help]
	autocorrect = 50
[apply]
	whitespace = nowarn
